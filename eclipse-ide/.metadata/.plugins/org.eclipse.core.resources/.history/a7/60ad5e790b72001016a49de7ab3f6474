package com.tom.backdoor.server.config;

import java.util.HashMap;
import java.util.Map;

import org.springframework.stereotype.Component;

import lombok.RequiredArgsConstructor;
import lombok.extern.log4j.Log4j2;

@Log4j2
@Component
@RequiredArgsConstructor
public class ConfigUtil {
	
	private Map<String, String> serverData = new HashMap<>();
	
	public void stopService(String serverName) {
		
		serverData.compute();
		
		
		log.info("Service Stopped from IP: {}, service: {}");
	}
	
	public void clearData(String serverName) {

		
		
		log.info("Data cleared from IP: {}, service: {}");
	}
	
}
